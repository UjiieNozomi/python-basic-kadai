        "# 関数\n",
        "def calculate_tax(price, tax):\n",
        "\n",
        "  sales_tax = price * (tax * 0.01)\n",
        "  total = price + sales_tax\n",
        "\n",
        "  return sales_tax, total\n",
        "\n",
        "calculate_tax(100,10)\n",
