      "source": [
        "class Human:\n",
        "\n",
        "  # コンストラクタ\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  # メソッド\n",
        "  def printinfo(self):\n",
        "    print(self.name, self.age)\n",
        "\n",
        "# インスタンス化\n",
        "person = Human(\"侍太郎\", \"23\")\n",
        "\n",
        "person.printinfo()\n",
        "\n",
        "\n"
      ]
